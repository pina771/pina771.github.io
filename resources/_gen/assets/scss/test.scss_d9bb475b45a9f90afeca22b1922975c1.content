.navbar{width:100%;position:sticky;top:0;z-index:30;box-shadow:0 4px 6px -1px rgba(0,0,0,.1),0 2px 4px -2px rgba(0,0,0,.1);background-color:#fff}.navbar ul{list-style:none;display:flex}.footer{position:absolute;bottom:0;left:0;right:0;margin-left:auto;margin-right:auto;width:fit-content;background-color:#708090;-webkit-clip-path:polygon(5% 0, 95% 0, 100% 100%, 0% 100%);clip-path:polygon(5% 0, 95% 0, 100% 100%, 0% 100%)}.footer ul{list-style:none;display:flex;justify-content:center;padding:0px 12px;gap:12px}.footer ul li{padding:12px}.footer ul li a{text-decoration:none;color:#000;font-family:"Fira Code",monospace}