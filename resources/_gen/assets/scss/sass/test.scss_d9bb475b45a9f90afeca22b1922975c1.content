.navbar{padding:16px 0 0 0;width:100%;z-index:50;position:sticky;top:0;left:0;transition:top .5s ease}.navbar ul{display:flex;justify-content:center;align-items:center;list-style:none;gap:16px}.navbar ul li a{display:flex;justify-content:center;align-items:center;padding:16px;color:#20201e;text-decoration:none;font-family:"Fira Code",monospace;border-radius:12px;box-shadow:0 10px 15px -3px rgba(0,0,0,.1),0 4px 6px -4px rgba(0,0,0,.1);transition:background-color .2s ease}.navbar ul li a:hover{background-color:#c3e7ed;text-decoration:underline}@media screen and (max-width: 768px){.navbar ul li a{background-color:#c3e7ed}}.main{display:flex;flex-direction:column;justify-content:center;align-items:center;margin:auto}.main .top{display:flex;padding:24px;justify-content:center;align-items:center;gap:12px}@media screen and (max-width: 600px){.main .top{flex-direction:column}}.main .top .info{margin-top:16px;display:flex;flex-direction:column}.main .top .info h1{font-family:"Fira Code",monospace;color:#073b4c;font-size:32px}.main .top .info h3{font-weight:normal;font-size:24px}.main .top .image-container{position:relative;width:144px;height:144px}.main .top .image-container img{position:absolute;top:4px;left:8px;width:128px;height:128px;object-fit:cover;border-radius:50%;z-index:10}.main .top .image-container .image-flair{position:absolute;top:0;left:0;width:144px;height:144px;border-radius:51% 49% 34% 66%/27% 67% 33% 73%;background:linear-gradient(to bottom right, #073b4c, #ea526f);z-index:3}.main .bottom{display:flex;justify-content:center;align-items:center}.main .bottom .bottom-left{flex:0 1 640px;display:flex;flex-direction:column;justify-content:center;align-items:flex-start;padding:12px;gap:24px;border-right:1px solid gray}.main .bottom .bottom-left .description-long{font-size:18px}.main .bottom .bottom-left .description-long a{color:#20201e;text-decoration:none;font-family:"Fira Code",monospace;text-decoration:underline}.main .bottom .bottom-left .description-long a:hover{font-weight:bold}.main .bottom .bottom-left .stack-title{display:flex;align-items:center;margin-bottom:8px}.main .bottom .bottom-left .stack h4{font-weight:normal;font-size:24px}.main .bottom .bottom-left .stack ul{list-style:none;display:flex;flex-direction:column;gap:12px;padding-left:16px}.main .bottom .bottom-left .stack ul li{font-size:18px}.main .bottom .bottom-left .stack ul li span{font-weight:bold;background-color:#c3e7ed}@media screen and (max-width: 600px){.main .bottom .bottom-left{flex:0 1}.main .bottom .bottom-left .stack{padding-bottom:12px;border-bottom:solid 1px gray}}.main .bottom .contact{flex:0 1;padding:24px;display:flex;flex-direction:column;justify-content:center;align-items:start;color:#20201e}.main .bottom .contact h4{font-weight:normal;font-size:24px;margin-bottom:8px}.main .bottom .contact ul{list-style:none;display:flex;flex-direction:column;gap:4px}.main .bottom .contact ul li a{font-size:18px;color:#20201e;text-decoration:none;font-family:"Fira Code",monospace}.main .bottom .contact ul li a:hover{font-weight:bold;text-decoration:underline}@media screen and (max-width: 600px){.main .bottom{flex-direction:column}.main .bottom .bottom-left{border-right:none}}